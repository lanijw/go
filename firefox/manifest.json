{
  "manifest_version": 3,
  "name": "go",
  "description": "Shortlink Manager",
  "version": "1.4.0",
  "omnibox": {
    "keyword": "go"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "img/go-logo.png"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "background": {
    "scripts": ["service-worker.js"],
    "type": "module"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "go@laniw.com",
      "strict_min_version": "42.0"
    }
  }
}
